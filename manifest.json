{
  "manifest_version": 2,
  "name": "History Keyword Filter",
  "version": "1.0",
  "permissions": ["history", "storage"],
  "background": {
    "scripts": ["dist/background.js"]
  },
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": true
  },
  "browser_action": {
    "default_popup": "src/popup.html"
  }
}
